generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" // or "mysql" / "sqlite"
  url      = env("DATABASE_URL")
}

model Message {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  text      String
  createdAt DateTime
  sender    String   @default("Anonymous")
  timestamp DateTime @default(now())
}
